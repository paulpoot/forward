{
  "name": "forward",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "bundlesize": "bundlesize",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "test:coverage:open": "open-cli ./coverage/lcov-report/index.html",
    "prettier": "prettier --check \"src/**/*.tsx\" \"src/**/*.ts\" \".storybook/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\" \".storybook/**/*.js\"",
    "stylelint": "stylelint \"*.scss\" \"**/*.scss\"",
    "stylelint:fix": "stylelint \"*.scss\" \"**/*.scss\" --fix",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public -s ./src/assets/fonts",
    "build-storybook": "build-storybook -s ./public -s ./src/assets/fonts"
  },
